{
  "name": "code-gen-server",
  "version": "1.0.0",
  "description": "Server to support the code generation electron app",
  "main": "index.ts",
  "author": "Kevin Grassi",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "18.x",
    "npm": ">=6"
  },
  "scripts": {
    "prestart": "yarn run build",
    "start": "node dist/server.js",
    "preserve": "yarn run build",
    "serve": "concurrently \"npx tsc -w\" \"nodemon dist/server.js\"",
    "generate-db-types": "npx supabase gen types typescript --project-id xexjtohvdexqxpomspdb --schema public > types/supabase.ts",
    "tsc": "./node_modules/typescript/bin/tsc",
    "build": "tsc",
    "postinstall": "npm run build",
    "test": "jest --watchAll --verbose"
  },
  "dependencies": {
    "@slack/bolt": "^3.13.0",
    "@slack/web-api": "^6.8.1",
    "@supabase/supabase-js": "^2.10.0",
    "@types/jest": "^29.5.1",
    "axios": "^1.3.5",
    "body-parser": "^1.20.2",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "gpt-3-encoder": "^1.1.4",
    "jest": "^29.5.0",
    "limiter": "^2.1.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.2",
    "openai": "^3.2.1",
    "rimraf": "^4.4.0",
    "simple-git": "^3.17.0",
    "sinon": "^15.0.4",
    "tree-sitter": "^0.20.1",
    "tree-sitter-java": "^0.19.1",
    "tree-sitter-python": "^0.20.1",
    "tree-sitter-typescript": "^0.20.1",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/node-cron": "^3.0.7",
    "@types/sinon": "^10.0.14",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  }
}
