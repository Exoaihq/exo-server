{
  "name": "code-gen-server",
  "version": "1.0.0",
  "description": "Server to support the code generation electron app",
  "main": "index.ts",
  "author": "Kevin Grassi",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/server.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\" \"nodemon dist/server.js\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.10.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "rimraf": "^4.4.0",
    "tree-sitter": "^0.20.1",
    "tree-sitter-typescript": "^0.20.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "nodemon": "^2.0.21",
    "typescript": "^4.9.5"
  }
}
